{
  "/api/v4/user-libraries/": {
    "get": {
      "summary": "Get a list of user libraries",
      "operationId": "getUserLibraries",
      "tags": [
        "UserLibrary"
      ],
      "responses": {
        "200": {
          "description": "A list of user libraries",
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/UserLibrary"
                }
              }
            }
          }
        }
      }
    },
    "post": {
      "summary": "Create a new user library",
      "operationId": "createUserLibrary",
      "tags": [
        "UserLibrary"
      ],
      "requestBody": {
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/UserLibraryCreate"
            }
          }
        },
        "required": true
      },
      "responses": {
        "201": {
          "description": "User library created",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UserLibrary"
              }
            }
          }
        }
      }
    }
  },
  "/api/v4/user-libraries/{id}/": {
    "get": {
      "summary": "Get a user library by ID",
      "operationId": "getUserLibraryById",
      "tags": [
        "UserLibrary"
      ],
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "required": true,
          "schema": {
            "type": "integer"
          }
        }
      ],
      "responses": {
        "200": {
          "description": "A single user library",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UserLibrary"
              }
            }
          }
        },
        "404": {
          "description": "User library not found"
        }
      }
    },
    "put": {
      "summary": "Update a user library by ID",
      "operationId": "updateUserLibraryById",
      "tags": [
        "UserLibrary"
      ],
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "required": true,
          "schema": {
            "type": "integer"
          }
        }
      ],
      "requestBody": {
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/UserLibraryCreate"
            }
          }
        },
        "required": true
      },
      "responses": {
        "200": {
          "description": "User library updated",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UserLibrary"
              }
            }
          }
        },
        "404": {
          "description": "User library not found"
        }
      }
    },
    "delete": {
      "summary": "Delete a user library by ID",
      "operationId": "deleteUserLibraryById",
      "tags": [
        "UserLibrary"
      ],
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "required": true,
          "schema": {
            "type": "integer"
          }
        }
      ],
      "responses": {
        "204": {
          "description": "User library deleted"
        },
        "404": {
          "description": "User library not found"
        }
      }
    }
  }
}